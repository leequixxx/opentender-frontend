{
  "name": "opentender-portal-frontend",
  "version": "0.8.3",
  "description": "Opentender Portal Frontend (Server/Client)",
  "author": "okfde",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/digiwhist/opentender-frontend.git"
  },
  "scripts": {
    "start": "npm run server",
    "watch": "webpack --watch --display-error-details",
    "prebuild": "rimraf dist",
    "build": "npm run build:styles:prod && npm run build:universal:prod && npm run build:app-client:prod && npm run build:server:prod",
    "server": "node dist/server/index.js",
    "debug": "node --debug-brk dist/server/index.js",
    "lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "schema": "cd bin && node schema2ts.js",
    "langs": "cd bin && node languages2files.js",
    "docs": "compodoc -p tsconfig.compodoc.json -n 'Opentender Portal Frontend' -d ./dist/docs/",
    "develop": "concurrently --kill-others \"npm run app-server\" \"npm run watch:universal\" \"npm run watch:styles\" \"npm run watch:server\" \"npm run run:nodemon\"",
    "run:nodemon": "nodemon dist/server/index.js",
    "watch:app-client": "webpack --watch --display-error-details --config webpack.app-client.js",
    "watch:app-server": "webpack --watch --display-error-details --config webpack.app-server.js",
    "watch:standalone": "ng build --watch --app 0",
    "watch:universal": "ng build --watch --app 1",
    "watch:styles": "webpack --watch --display-error-details --config webpack.styles.js",
    "watch:server": "webpack --watch --display-error-details --config webpack.server.js",
    "bundle-report": "npm run build:stats && webpack-bundle-analyzer --mode static dist/standalone/stats.json",
    "build:stats": "ng build --named-chunks --aot=false --app 0 --prod --stats-json --output-hashing=false",
    "build:server": "webpack --config webpack.server.js",
    "build:server:prod": "webpack -p --config webpack.server.js",
    "build:styles": "webpack --config webpack.styles.js",
    "build:styles:prod": "webpack -p --config webpack.styles.js",
    "build:app-client": "webpack --progress --config webpack.app-client.js",
    "build:app-client:prod": "webpack --progress -p --config webpack.app-client.js",
    "build:app-server": "webpack --progress --config webpack.app-server.js",
    "build:app-server:prod": "webpack --progress -p --config webpack.app-server.js",
    "build:standalone": "ng build --aot=false --app 0 --output-hashing=false",
    "build:standalone:prod": "ng build --prod --aot=false --app 0",
    "build:universal": "ng build --aot=false --app 1 --output-hashing=false",
    "build:universal:prod": "ng build --prod --aot=false --app 1 --output-hashing=false"
  },
