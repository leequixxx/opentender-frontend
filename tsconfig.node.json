{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/server",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "module": "commonjs",
    "types": [],
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es5",
      "es2016",
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
